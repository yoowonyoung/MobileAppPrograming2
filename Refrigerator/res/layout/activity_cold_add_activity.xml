<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/relativeLayout"
        android:layout_alignParentBottom="true"
        >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="59dp"
            android:id="@+id/imageView3"
            android:background="@drawable/bar1"

            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="식료품 추가_ 냉동실 "
        android:layout_margin="16dp"
        android:textColor="#fffffff7"
        android:textStyle="bold"
        android:textSize="20sp"

        android:id="@+id/textView2"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">



            <RelativeLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/pp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/re23"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <RelativeLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:id="@+id/re99"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true">

                            <RelativeLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:id="@+id/re90"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true">

                                <RelativeLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentLeft="true"
                                    android:layout_alignParentStart="true">

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="61sp"
                                        android:id="@+id/imageView5"
                                        android:background="@drawable/under2"
                                        android:layout_marginTop="55dp"
                                        android:layout_alignParentTop="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="42dp"
                                        android:id="@+id/imageView4"


                                        android:background="@drawable/menu1123"
                                        android:layout_below="@+id/imageView5"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <Spinner
                                        android:layout_width="23dp"
                                        android:layout_height="23dp"
                                        android:id="@+id/spinner1"
                                        android:prompt="@string/promt_menu"
                                        android:entries="@array/menu"
                                        android:background="@drawable/folder2"
                                        android:layout_marginRight="30dp"
                                        android:layout_marginEnd="30dp"
                                        android:layout_marginTop="8dp"
                                        android:layout_alignTop="@+id/imageView4"
                                        android:layout_alignParentRight="true"
                                        android:layout_alignParentEnd="true" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="90sp"
                                        android:id="@+id/imageView8"
                                        android:background="@drawable/chi"
                                        android:layout_below="@+id/imageView4"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="42dp"
                                        android:id="@+id/imageView6"
                                        android:background="@drawable/information5"
                                        android:layout_below="@+id/imageView8"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="200dp"
                                        android:id="@+id/imageView7"
                                        android:background="@drawable/test15"
                                        android:layout_below="@+id/imageView6"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <EditText
                                        android:layout_width="90dp"
                                        android:layout_height="32dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"

                                        android:hint="name"
                                        android:id="@+id/textView3"
                                        android:layout_marginTop="19dp"
                                        android:layout_marginRight="10dp"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_marginLeft="129dp"
                                        android:layout_marginStart="129dp"
                                        android:layout_alignTop="@+id/imageView7"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true" />

                                    <EditText
                                        android:layout_width="60dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText4"
                                        android:layout_marginTop="28dp"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_below="@+id/textView3"
                                        android:layout_centerHorizontal="true" />

                                    <EditText
                                        android:layout_width="40dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText5"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_marginLeft="26dp"
                                        android:layout_marginStart="26dp"
                                        android:layout_above="@+id/editText6"
                                        android:layout_toRightOf="@+id/editText4"
                                        android:layout_toEndOf="@+id/editText4" />

                                    <EditText
                                        android:layout_width="60dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText6"
                                        android:layout_marginTop="19dp"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_below="@+id/editText4"
                                        android:layout_toLeftOf="@+id/editText5"
                                        android:layout_toStartOf="@+id/editText5" />

                                    <EditText
                                        android:layout_width="40dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText7"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_alignTop="@+id/editText5"
                                        android:layout_alignRight="@+id/spinner1"
                                        android:layout_alignEnd="@+id/spinner1" />

                                    <EditText
                                        android:layout_width="40dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText8"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_alignTop="@+id/editText6"
                                        android:layout_alignLeft="@+id/editText5"
                                        android:layout_alignStart="@+id/editText5" />

                                    <EditText
                                        android:layout_width="40dp"
                                        android:layout_height="30dp"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:hint=""
                                        android:id="@+id/editText9"
                                        android:background="@drawable/abc_popup_background_mtrl_mult"
                                        android:layout_alignTop="@+id/editText8"
                                        android:layout_alignLeft="@+id/editText7"
                                        android:layout_alignStart="@+id/editText7" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="41dp"
                                        android:id="@+id/imageView9"
                                        android:layout_below="@+id/imageView7"
                                        android:layout_alignParentLeft="true"
                                        android:layout_alignParentStart="true"
                                        android:background="@drawable/al6" />

                                    <ToggleButton
                                        android:layout_width="wrap_content"
                                        android:layout_height="40dp"
                                        android:text="New ToggleButton"
                                        android:id="@+id/toggleButton"
                                        android:layout_below="@+id/imageView9"
                                        android:layout_alignRight="@+id/editText9"
                                        android:layout_alignEnd="@+id/editText9" />

                                    <ImageView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:id="@+id/imageView10"
                                        android:layout_alignParentBottom="true"
                                        android:layout_toStartOf="@+id/toggleButton"
                                        android:background="@drawable/white"
                                        android:layout_alignParentRight="true"
                                        android:layout_alignParentEnd="true"
                                        android:layout_below="@+id/imageView9" />

                                </RelativeLayout>

                            </RelativeLayout>

                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="657dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp">

                </RelativeLayout>

                <ImageButton
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageButton6"
                    android:onClick="onClick"
                    android:background="@drawable/home1"
                    android:layout_marginTop="16dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="28dp"
                    android:layout_marginEnd="28dp" />

            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>



</RelativeLayout>